---
import "./styles/video.css";
const { id } = Astro.props;
---

<div
  class="videoContainer relative w-full sm:w-full mx-auto z-40 h-screen flex flex-col items-center justify-center"
>
  <div class="absolute top-0 left-0 -translate-y-full preVideo w-screen h-1/3"></div>
  <div id={id} class="w-full sm:w-2/3 aspect-video bg-pink-50"></div>
</div>
