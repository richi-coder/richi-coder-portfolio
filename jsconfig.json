{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@services/*": ["src/services/*"],
      "@modules/*": ["src/modules/*"],
      "@scripts/*": ["src/scripts/*"], 
      "@styles/*": ["src/styles/*"],
    }
  }
}